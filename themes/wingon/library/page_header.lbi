$(document).ready(function(){
	$(".nav-sec").hover(
		function(){
			$(".dropdown",this).css('left','-2px');
			$(".dropdown",this).css('width','100%');
			$(".dropdown",this).css('z-index','99999999');
			$(this).addClass('selected');
